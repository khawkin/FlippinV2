<p id="notice"><%= notice %></p>

<h1>Listing Products</h1>

<table>
  <thead>
    <tr>
      <th>Industry</th>
      <th>Third party</th>
      <th>Name</th>
      <th>Averagenewprice</th>
      <th>Newprice</th>
      <th>Averageusedprice</th>
      <th>Usedprice</th>
      <th>Quality</th>
      <th>Productcredibility</th>
      <th>Productlocation</th>
      <th>Type</th>
      <th>Age</th>
      <th>Sellingtime</th>
      <th>Bidprice?</th>
      <th>Description</th>
      <th>Quantity</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @products.each do |product| %>
      <tr>
        <td><%= product.industry_id %></td>
        <td><%= product.third_party_id %></td>
        <td><%= product.name %></td>
        <td><%= product.averageNewPrice %></td>
        <td><%= product.newPrice %></td>
        <td><%= product.averageUsedPrice %></td>
        <td><%= product.usedPrice %></td>
        <td><%= product.quality %></td>
        <td><%= product.productCredibility %></td>
        <td><%= product.productLocation %></td>
        <td><%= product.type %></td>
        <td><%= product.age %></td>
        <td><%= product.sellingTime %></td>
        <td><%= product.bidPrice? %></td>
        <td><%= product.description %></td>
        <td><%= product.quantity %></td>
        <td><%= link_to 'Show', product %></td>
        <td><%= link_to 'Edit', edit_product_path(product) %></td>
        <td><%= link_to 'Destroy', product, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product', new_product_path %>
